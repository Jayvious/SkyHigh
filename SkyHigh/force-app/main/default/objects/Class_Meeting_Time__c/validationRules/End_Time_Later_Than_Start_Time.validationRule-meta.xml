<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Time_Later_Than_Start_Time</fullName>
    <active>true</active>
    <description>Class must end after it starts.</description>
    <errorConditionFormula>End_Time__c &lt;  Start_Time__c</errorConditionFormula>
    <errorDisplayField>End_Time__c</errorDisplayField>
    <errorMessage>The class end time cannot be earlier than its start time</errorMessage>
</ValidationRule>
